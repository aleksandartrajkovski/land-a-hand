+    font-family: 'Fira Sans', sans-serif; /* Your chosen font */
+}
+.about-title {
+    font-size: 4rem; /* Big title size */
+    margin-bottom: 2rem; /* Space under title */
+    text-align: center; /* Center align title */
+}
+
+.about-description {
+    font-size: 1.5rem; /* Description text size */
+    margin-bottom: 2rem; /* Space under description */
+    text-align: center; /* Center align description */
+}
+
+.about-container {
+    flex-direction: column; /* Stack children vertically */
+    align-items: center; /* Center items on cross axis */
+    justify-content: center; /* Center items on main axis */
+    max-width: 1200px; /* Maximum width of content */
+    margin: auto; /* Center container */
+    display: flex;
+}
+
+
