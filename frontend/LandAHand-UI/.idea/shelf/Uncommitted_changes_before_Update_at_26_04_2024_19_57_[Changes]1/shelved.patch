+    background: #8CCC88;
+    color: white;
+    padding: 1rem 2rem;
+    border: none;
+    border-radius: 50px;
+    font-size: 1.25rem;
+    cursor: pointer;
+    text-align: center;
+    margin: 2rem auto; /* Space around button and center */
+}
+.click-here {
+    position: absolute;
+    width: auto;
+    height: auto;
+    right: 5%; /* Adjust right position as needed */
+    bottom: 2%; /* Adjust bottom position as needed */
+    font-family: 'Fira Sans', sans-serif;
+    font-size: 1.3vw; /* Adjust font size for responsiveness */
+    color: #FDFDFD;
+}
+
+.woman-hugging-dog {
+    float: right;
+    position: absolute;
+    left: 500px;
+    z-index: 10; /* This should be higher than the rectangles' z-index */
+    transform: scale(0.9);
+    bottom: 0px;
+}
